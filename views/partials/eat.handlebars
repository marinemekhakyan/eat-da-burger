  <p class = "burgers">
    {{#if devoured}}
    <button class="eatButton" id="tryBurger" data-id="{{burger_id}}" data-eaten="{{devoured}}">
        Devour
    </button>
    {{else}}
    <button class="eatButton" id="eatBurger" data-id="{{burger_id}}" data-eaten="{{devoured}}">
        Eat Burger!
    </button>
    {{/if}}
    {{burger_name}}
</p>